#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/creatrip/Documents/chrome-extension-boilerplate-react-vite/node_modules/.pnpm/vitest@1.3.1_@types+node@20.8.10_jsdom@22.1.0_sass@1.69.5/node_modules/vitest/node_modules:/Users/creatrip/Documents/chrome-extension-boilerplate-react-vite/node_modules/.pnpm/vitest@1.3.1_@types+node@20.8.10_jsdom@22.1.0_sass@1.69.5/node_modules:/Users/creatrip/Documents/chrome-extension-boilerplate-react-vite/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/creatrip/Documents/chrome-extension-boilerplate-react-vite/node_modules/.pnpm/vitest@1.3.1_@types+node@20.8.10_jsdom@22.1.0_sass@1.69.5/node_modules/vitest/node_modules:/Users/creatrip/Documents/chrome-extension-boilerplate-react-vite/node_modules/.pnpm/vitest@1.3.1_@types+node@20.8.10_jsdom@22.1.0_sass@1.69.5/node_modules:/Users/creatrip/Documents/chrome-extension-boilerplate-react-vite/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
